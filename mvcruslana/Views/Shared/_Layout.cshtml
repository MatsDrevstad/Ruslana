                    </div>
                </div>

                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Hymn</h3>
                    </div>
                    <div class="panel-body">
                        <iframe class="img-responsive" src="http://www.youtube.com/embed/dJeLiPnz6E8?rel=0&fs=1" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
                    </div>
                </div>

            </div>
            <div class="col-md-8">
                @RenderBody()
            </div>
            <div class="col-md-2">

                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Tvopit' UAVisti!</h3>
                    </div>
                    <div class="panel-body">
                            <h3>Tvopit' UAVisti!</h3>
                        <p>
